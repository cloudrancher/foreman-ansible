---
# {{ ansible_managed }}

# Can be true, false, or http/https to enable just one of the protocols
:enabled: {{ foreman_proxy_dns }}

# Valid providers:
#   dns_nsupdate
#   dns_nsupdate_gss (for GSS-TSIG support)
#   dns_libvirt
#   dns_dnscmd
:use_provider: {{ foreman_proxy_dns_provider }}
# use this setting if you want to override default TTL setting (86400)
:dns_ttl: {{ foreman_proxy_dns_ttl }}
:dns_key: {{ foreman_proxy_dns_key_file }}
:dns_server: {{ foreman_proxy_dns_server }}
